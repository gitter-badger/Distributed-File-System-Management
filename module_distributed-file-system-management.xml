<?xml version="1.0" encoding="UTF-8"?>
<project name="module_distributed-file-system-management" default="compile.module.distributed-file-system-management">
  <dirname property="module.distributed-file-system-management.basedir" file="${ant.file.module_distributed-file-system-management}"/>
  
  <property name="module.jdk.home.distributed-file-system-management" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.distributed-file-system-management" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.distributed-file-system-management" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.distributed-file-system-management" value="-encoding UTF-8 -source 15 -target 15 ${compiler.args}"/>
  
  <property name="distributed-file-system-management.output.dir" value="${module.distributed-file-system-management.basedir}/out/production/Distributed-File-System-Management"/>
  <property name="distributed-file-system-management.testoutput.dir" value="${module.distributed-file-system-management.basedir}/out/test/Distributed-File-System-Management"/>
  
  <path id="distributed-file-system-management.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="distributed-file-system-management.module.production.classpath">
    <path refid="${module.jdk.classpath.distributed-file-system-management}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.commons-io-2.8.0.classpath"/>
    <path refid="library.lombok.classpath"/>
  </path>
  
  <path id="distributed-file-system-management.runtime.production.module.classpath">
    <pathelement location="${distributed-file-system-management.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.commons-io-2.8.0.classpath"/>
    <path refid="library.lombok.classpath"/>
  </path>
  
  <path id="distributed-file-system-management.module.classpath">
    <path refid="${module.jdk.classpath.distributed-file-system-management}"/>
    <pathelement location="${distributed-file-system-management.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.commons-io-2.8.0.classpath"/>
    <path refid="library.lombok.classpath"/>
  </path>
  
  <path id="distributed-file-system-management.runtime.module.classpath">
    <pathelement location="${distributed-file-system-management.testoutput.dir}"/>
    <pathelement location="${distributed-file-system-management.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.commons-io-2.8.0.classpath"/>
    <path refid="library.lombok.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.distributed-file-system-management">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.distributed-file-system-management">
    <patternset refid="excluded.from.module.distributed-file-system-management"/>
  </patternset>
  
  <path id="distributed-file-system-management.module.sourcepath">
    <dirset dir="${module.distributed-file-system-management.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.distributed-file-system-management" depends="compile.module.distributed-file-system-management.production,compile.module.distributed-file-system-management.tests" description="Compile module Distributed-File-System-Management"/>
  
  <target name="compile.module.distributed-file-system-management.production" depends="register.custom.compilers" description="Compile module Distributed-File-System-Management; production classes">
    <mkdir dir="${distributed-file-system-management.output.dir}"/>
    <javac2 destdir="${distributed-file-system-management.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.distributed-file-system-management}/javac">
      <compilerarg line="${compiler.args.distributed-file-system-management}"/>
      <bootclasspath refid="distributed-file-system-management.module.bootclasspath"/>
      <classpath refid="distributed-file-system-management.module.production.classpath"/>
      <src refid="distributed-file-system-management.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.distributed-file-system-management"/>
    </javac2>
    
    <copy todir="${distributed-file-system-management.output.dir}">
      <fileset dir="${module.distributed-file-system-management.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.distributed-file-system-management.tests" depends="register.custom.compilers,compile.module.distributed-file-system-management.production" description="compile module Distributed-File-System-Management; test classes" unless="skip.tests"/>
  
  <target name="clean.module.distributed-file-system-management" description="cleanup module">
    <delete dir="${distributed-file-system-management.output.dir}"/>
    <delete dir="${distributed-file-system-management.testoutput.dir}"/>
  </target>
</project>